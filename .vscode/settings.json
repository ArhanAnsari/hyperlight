{
  "files.associations": {
    "*.h": "c",
},
  "rust-analyzer.showUnlinkedFileNotification": false,
  "rust-analyzer.linkedProjects": [
    "Cargo.toml",
    "src/hyperlight_host/Cargo.toml",
    "src/hyperlight_guest/Cargo.toml",
    "src/hyperlight_testing/Cargo.toml",
    "src/hyperlight_common/Cargo.toml",
    "src/hyperlight_guest_capi/Cargo.toml",
    "src/tests/rust_guests/dummyguest/Cargo.toml",
    "src/tests/rust_guests/simpleguest/Cargo.toml",
    "src/tests/rust_guests/callbackguest/Cargo.toml"
  ],
  "rust-analyzer.rustfmt.extraArgs": [
    "+nightly" // required for rustfmt.toml which uses nightly features
  ],
  "githubPullRequests.ignoredPullRequestBranches": [
    "dev"
  ]
}